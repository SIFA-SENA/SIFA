<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  
  <display-name>gestion_fichas</display-name>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  
  <servlet>
    <servlet-name>controller.Usuario</servlet-name>
    <servlet-class>controller.Usuario</servlet-class>
  </servlet>
   <servlet-mapping> 
    <servlet-name>controller.Usuario</servlet-name>
    <url-pattern>/usuario</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>controller.Ficha</servlet-name>
    <servlet-class>controller.Ficha</servlet-class>
  </servlet>
   <servlet-mapping> 
    <servlet-name>controller.Ficha</servlet-name>
    <url-pattern>/ficha</url-pattern>
  </servlet-mapping>


   <servlet>
    <servlet-name>controller.Aprendiz</servlet-name>
    <servlet-class>controller.Aprendiz</servlet-class>
  </servlet>
   <servlet-mapping> 
    <servlet-name>controller.Aprendiz</servlet-name>
    <url-pattern>/aprendiz</url-pattern>
  </servlet-mapping>

</web-app>
